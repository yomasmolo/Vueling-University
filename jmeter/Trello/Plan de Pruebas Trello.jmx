function generarFraseCorta(cantidadPalabras) {
  var frase = &quot;&quot;;
  for (var i = 0; i &lt; cantidadPalabras; i++) {
    var indice = Math.floor(Math.random() * palabras.length);
    frase += palabras[indice] + &quot; &quot;;
  }
  return frase.trim();
}

var fraseAleatoria = generarFraseCorta(3);
vars.put(&quot;fraseAleatoria&quot;, fraseAleatoria);</stringProp>
